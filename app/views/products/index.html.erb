<h1>Products</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Stock Quantity</th>
      <th>Category</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= link_to product.title, product_path(product) %></td>
        <td><%= product.price %></td>
        <td><%= product.stock_quantity %></td>
        <td><%= product.category.name %></td>
        <td><%= link_to 'Show', product_path(product) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
